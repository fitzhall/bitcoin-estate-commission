{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "env": {
    "SKIP_ENV_VALIDATION": "true"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "true",
      "SKIP_DATABASE_DURING_BUILD": "true"
    }
  },
  "functions": {
    "app/**/*.js": {
      "maxDuration": 60
    }
  },
  "regions": ["iad1"]
}